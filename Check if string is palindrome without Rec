public class Rec_6 {

        public static void main (String[] args){
        String s="MADAM";
        int n= s.length();
        //create empty string
        String ans="";
        for(int i=n-1;i>=0;i--){
            ans=ans+s.charAt(i);
        }
        if(s.equals(ans)){
            System.out.println("Yes");
        }
        else{
            System.out.println("No");
        }
        }
    }
